			END
		  	LPF ADD_SPELL_EFFECT INT_VAR insert_point = 0 opcode = 172 target = 1 timing = 9 STR_VAR resource = EVAL ~%cast_spell%~ END
		  COPY_EXISTING ~d5srci4.spl~ ~override~
			PATCH_IF spell_level = 4 BEGIN
			  LPF ADD_SPELL_EFFECT INT_VAR opcode = 177 target = 1 parameter2 = 2 timing = 1 STR_VAR resource = EVAL ~%spell_eff%~ END
			END
		  	LPF ADD_SPELL_EFFECT INT_VAR insert_point = 0 opcode = 172 target = 1 timing = 9 STR_VAR resource = EVAL ~%cast_spell%~ END
		  COPY_EXISTING ~d5srci5.spl~ ~override~
