//get kit identifier
//
COPY_EXISTING ~kit.ids~ ~override~
  COUNT_2DA_COLS cols 
  READ_2DA_ENTRIES_NOW rows cols 
  FOR (row = 1; row < rows; ++row) BEGIN 
    READ_2DA_ENTRY_FORMER rows row 1 ~kit_name~ 
    PATCH_IF ~%kit_name%~ STRING_EQUAL_CASE ~D5_INCARNATE_MYSTRA~ BEGIN
      SET fake_sorcerer_row = %row%
      READ_2DA_ENTRY_FORMER rows fake_sorcerer_row 0 fake_sorcerer_code
    END
  END
BUT_ONLY


