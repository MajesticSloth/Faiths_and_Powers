
IF
	NumTimesTalkedTo(0)
THEN
	RESPONSE #100
	SetNumTimesTalkedTo(1)
	SetGlobal("D5FPSPNT","GLOBAL",1)
	ActionOverride(LastSummonerOf(Myself),StartDialogOverride("d5fpsp2",Myself))
END

IF
	True()
THEN
	RESPONSE #100
	SetGlobal("D5FPSPNT","GLOBAL",0)
	SetNumTimesTalkedTo(0)
	DestroySelf() 
END
