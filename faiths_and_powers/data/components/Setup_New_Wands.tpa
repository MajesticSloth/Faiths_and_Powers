//| of Banishment (Ns) | ? | Removes summoned creatures, evil outsiders save or destroyed/banished |
//only installed if SR is installed 
ACTION_IF (FILE_CONTAINS_EVALUATED(~spell.ids~ ~CLERIC_BANISHMENT~)) BEGIN  //SR Banishment

 LAF RES_NUM_OF_SPELL_NAME
  STR_VAR spell_name = ~CLERIC_BANISHMENT~
  RET spell_res
 END
 COPY_EXISTING ~%spell_res%.spl~ ~override/B_WANBA.spl~
  WRITE_BYTE 0x001c  4  //innate
  LPF ALTER_EFFECT
      INT_VAR match_opcode = 177 savingthrow = 8           //  save wand
  END      //
  LPF CLONE_EFFECT 
      INT_VAR match_opcode = 177 savingthrow = 0  timing = 0 duration = 18
      STR_VAR resource = ~B_WANST~ END  //Stun
  END

  //stun eff
  COPY ~%mod_folder%/data/wands/B_WANST.eff~ ~override~   //stun effect

  //actual wand
  COPY ~%mod_folder%/data/wands/B_WAN12.itm~ ~override~
        SAY NAME2 @900010023
        SAY DESC @900010024


END
